{
  "name": "omed",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui": "pnpm --filter @omed/ui ui",
    "dash": "turbo dev --filter=@omed/dashboard",
    "web": "turbo dev --filter=@omed/web"
  },
  "devDependencies": {
    "@omed/eslint-config": "workspace:*",
    "@omed/typescript-config": "workspace:*",
    "prettier": "^3.5.1",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": "22.x"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "core-js-pure",
      "esbuild",
      "prisma",
      "sharp"
    ]
  }
}
